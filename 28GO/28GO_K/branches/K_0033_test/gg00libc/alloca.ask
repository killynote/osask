segment CODE(USE32, PARA);
default(code == CODE);
asmout("[FILE 'alloca.ask']");

asmout("GLOBAL __alloca");

void __alloca()
{
	PUSH(ECX);
	EAX =- EAX;
	LEA(ECX, [SS:ESP + EAX + 8]);
	EAX = ESP;
	ESP = ECX;
	ECX = [DS:EAX];
	JMP((int) [DS:EAX + 4]);
}
